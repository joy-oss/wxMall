.locationRow{
  height:95px;
}
.defLocation{
  position: relative;
}
.editAddr{
  width: 20px;
  height: 20px;
  position: absolute;
  right:10px;
  top:35px;
  padding-left:10px;
  font-size: 15px;
}
.showDefault{
  padding:15px;
  display: flex;
  flex-flow: column nowrap;
  font-size:14px;
}
.def{
  max-width: 250px;
  display:-webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  overflow: hidden;
  font-weight: bold;
  font-size: 16px;
}
.userInfo{
  display: flex;
  margin-top:10px;
}
.userInfo>view:nth-of-type(2){
  margin-left:10px;
}
.bboder{
  display:flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}
.bboder>view{
  width: 20px;
  height:3px;
}
.bboder>view:nth-of-type(2n){
  background-color: #759EF1;
}
.bboder>view:nth-of-type(2n+1){
  background-color: #F3715F;
}
.orderlist{
  padding:10px;
}
.orderInfo{
  display:flex;
  margin-bottom: 10px;
  border-bottom: solid 1px #F4F4F4;
  padding-bottom: 10px;
}
.orderInfo image{
  width: 100px;
  height: 100px;
  margin-right:10px;
  margin-top:10px;
}
.orderDetil{
  width: 220px;
}
.orderDetil>view{
  margin-bottom: 5px;
}
.goodsname{
  font-size:20px;
  font-weight: bold;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.goodsku{
  display: flex;
}
.goodsku>view{
  margin-right:5px;
  color:#ACACAC;
}
.goodsprice{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: flex-end;
  color:#E7746B;
  font-size: 20px;
  font-weight: bold;
}
.goodspriceitem{
  display: flex;
}
.goodsprice .pricelog{
  font-size: 15px;
  margin-top:4px ;
}
.goodsnum{
  color:#A9A9A9;
  font-size: 15px;
}
.server{
  width: 105px;
  padding:1px;
  font-size:12px;
  color:#E8AB9D;
  border:solid 1px #E8AB9D;
}
.packageInfo{
  position: relative;
  margin-top:-5px;
  margin-left:10px;
  margin-bottom: 30px;
}
.expressage{
  position: absolute;
  right:10px;
   top:0;
   text-align: right;
}
.expressage>view:nth-of-type(2){
  margin-top:4px;
}
.payInfo{
 padding-left:10px;
  border-top:5px solid #F5F5F5;
}
.payInfo .row{
  position: relative;
  margin-top:10px;
}
.payInfo .row>view:nth-of-type(2){
position: absolute;
font-weight: bold;
right:10px;
top:0;
}
.discount1>view:nth-of-type(2){
  color:#E7746B;
}
.discount2>view:nth-of-type(2){
  color:#B5B5B5;
  font-weight: lighter;
}
.bar{
  height:35px;
}
.pay{
  height:50px;
  width: 100%;
  position: relative;
}
.endPrice{
  display: flex;
  color:#E7746B;
  font-size: 25px;
  font-weight: bold;
  margin-top:5px;
}
.endPrice>view:nth-of-type(1){
  font-size: 18px;
  margin-top:7px;
}
.mypay{
  width: 80px;
  padding:5px 10px;
  margin-top:5px;
  border-radius: 20px;
  background-color: #F8381D;
  position: absolute;
  right:10px;
  top:0;
  text-align: center;
  color:#F8EDE0;
  font-weight: lighter;
}